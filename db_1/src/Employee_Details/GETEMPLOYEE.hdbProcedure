PROCEDURE "GETALLEMPDETAILS" (
    OUT result TABLE (
        "EmpId" INT,
        "EmpName" NVARCHAR(255),
        "EmpLoc" NVARCHAR(255),
        "Deptno" INT
    )
)
LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER
AS
BEGIN
    result = SELECT "EmpId", "EmpName", "EmpLoc", "Deptno"
             FROM Employee1;
END;
